{
  "name": "@chunkflow/upload-client-react",
  "version": "0.0.0",
  "description": "React adapter for ChunkFlow Upload SDK",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format esm --dts",
    "dev": "tsdown src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "upload",
    "react",
    "hooks",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@chunkflow/core": "workspace:*",
    "@chunkflow/protocol": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "fast-check": "^4.5.3",
    "jsdom": "^27.4.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
