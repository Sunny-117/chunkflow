{
  "name": "@chunkflow/upload-client-react",
  "version": "0.0.0",
  "description": "React adapter for ChunkFlow Upload SDK",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format esm --dts",
    "dev": "tsdown src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "upload",
    "react",
    "hooks",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@chunkflow/core": "workspace:*",
    "@chunkflow/protocol": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^18.3.0",
    "react": "^18.3.0",
    "tsdown": "^0.2.15",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
