{
  "name": "@chunkflow/upload-server",
  "version": "0.0.0",
  "description": "Server-side SDK for ChunkFlow Upload",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format esm --dts",
    "dev": "tsdown src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "upload",
    "server",
    "bff",
    "storage",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@chunkflow/protocol": "workspace:*",
    "jsonwebtoken": "^9.0.2",
    "spark-md5": "^3.0.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.0",
    "@types/spark-md5": "^3.0.4",
    "fast-check": "^3.19.0",
    "tsdown": "^0.2.15",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
