{
  "name": "@chunkflow/shared",
  "version": "0.0.0",
  "description": "Shared utilities and tools for ChunkFlow Upload SDK",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/index.ts --format esm --dts",
    "dev": "tsdown src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "upload",
    "utilities",
    "shared",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "mitt": "^3.0.1",
    "p-limit": "^5.0.0",
    "spark-md5": "^3.0.2"
  },
  "devDependencies": {
    "@types/spark-md5": "^3.0.4",
    "tsdown": "^0.2.15",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
