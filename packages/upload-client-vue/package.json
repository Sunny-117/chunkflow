{
  "name": "@chunkflow/upload-client-vue",
  "version": "0.0.0",
  "description": "Vue adapter for ChunkFlow Upload SDK",
  "keywords": [
    "composables",
    "typescript",
    "upload",
    "vue"
  ],
  "license": "MIT",
  "author": "",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsdown src/index.ts --format esm --dts",
    "dev": "tsdown src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@chunkflow/core": "workspace:*",
    "@chunkflow/protocol": "workspace:*"
  },
  "devDependencies": {
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.4.0",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vue": "^3.5.27"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
