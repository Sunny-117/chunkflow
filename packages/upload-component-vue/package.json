{
  "name": "@chunkflow/upload-component-vue",
  "version": "0.0.0",
  "description": "Vue UI components for ChunkFlow Upload SDK",
  "keywords": [
    "components",
    "typescript",
    "ui",
    "upload",
    "vue"
  ],
  "license": "MIT",
  "author": "",
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@chunkflow/core": "workspace:*",
    "@chunkflow/protocol": "workspace:*",
    "@chunkflow/shared": "workspace:*",
    "@chunkflow/upload-client-vue": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "unplugin-vue": "^5.2.0",
    "vite": "^6.0.11",
    "vitest": "^4.0.18",
    "vue": "^3.5.27",
    "vue-tsc": "^2.2.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
